fun println(const msg: f64) {}

struct Inner { public a: f64; public b: f64; }

struct Outer {
    public i: Inner;
    
    fun inner_sum(const this) -> f64 {
        return this.i.a + this.i.b;
    }

    fun take_inner(const this) -> Inner { 
        return this.i;
    }
}

fun main() {
    const o: Outer = Outer { 
        i = Inner {
            a = 2.0, 
            b = 3.0 
        } 
    };
    println(o.inner_sum());             // 5
}
