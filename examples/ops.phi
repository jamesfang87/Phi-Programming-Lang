fun main() {
    const a = (45 - (10 * 3)) + (6 * 2 / 4);
    const b = -(40 * (6 - 2));
    const c = a++;
    const d = ++a;
    println(a); // 20
    println(b); // -160
    println(c); // 18
    println(d); // 20

    if (false && a++ == 21) {
