struct Person {
    const name: string;
    var age: i64;
    const pos: Pos;

    fun age_gap(const other: i64) -> i64 {
        return other - age;
    }
}

struct Pos {
    const x: i64;
    const y: i64;
}

fun main() {
    var me: Person = Person {
        name = "james",
        age = 17,
        pos = Pos {
            x = 0,
            y = 0
        }
    };

    me.name;
    me.pos.x;
    me.age_gap(16);
}
