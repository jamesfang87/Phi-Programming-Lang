struct Person {
    public name: string;
    age: i64;
    pos: Pos;

    fun age_gap(const other: i64) -> i64 {
        return other - age;
    }

    public fun same_name(const other: Person) -> bool {
        return name == other.name;
    }
}

struct Pos {
    x: i64;
    y: i64;
}

fun main() {
    var me = Person {
        name = "james",
        age = 17,
        pos = Pos {
            x = 0,
            y = 0
        }
    };

    var phiona = Person {
        name = "phiona",
        age = 16,
        pos = Pos {
            x = 1,
            y = 1
        }
    };

    me.name;
    me.same_name(phiona);
}
