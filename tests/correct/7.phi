struct Person {
    public name: string;
    age: i64;
    pos: Pos;

    fun age_gap(const other: i64) -> i64 {
        return other - age;
    }
}

struct Pos {
    x: i64;
    y: i64;
}

fun main() {
    var me: Person = Person {
        name = "james",
        age = 17,
        pos = Pos {
            x = 0,
            y = 0
        }
    };

    me.name;
    me.age_gap(16);
}
